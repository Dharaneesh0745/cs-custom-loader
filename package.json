{
  "name": "cs-custom-loader",
  "version": "1.0.4",
  "type": "module",
  "author": "Dharaneesh Boobalan",
  "license": "ISC",
  "description": "A collection of customizable React 3D/Tailwind loaders.",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "react-loader",
    "react-loading",
    "tailwind loader",
    "tailwindcss spinner",
    "react spinner",
    "3D loader",
    "animated loader",
    "custom loader react",
    "tailwind react loader",
    "react typescript loader",
    "react animated spinner",
    "react loading indicator",
    "react ui spinner",
    "react loader package"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "build": "tsup src/components/index.tsx --format cjs,esm --dts",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/vite": "^4.1.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "tsup": "^7.2.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}